{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://rn-wallet-frontend.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "server.js"
    }
  ]
}